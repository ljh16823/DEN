<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ljh16.den">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".loading"> // 처음 시작하는 엑티비티를 loading으로 추가
            //엑티비티.xml에 intent를 추가하면 메인페스트.xml에 밑에 인텐트 함수들이 추가됨
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"> // 처음 시작하는 엑티비티를 loading으로 추가
            //엑티비티.xml에 intent를 추가하면 메인페스트.xml에 밑에 인텐트 함수들이 추가됨
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mapsuch"> </activity> //loading이 끝난 후 메인 엑티비티가 실행됨.

    </application>

</manifest>